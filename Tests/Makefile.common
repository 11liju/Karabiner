CXXFLAGS = -I../../ -I/usr/local/include \
    -Wall -Werror \
    -O2 \
    -std=c++11 \
    -DFLAGSTATUS_TEST

LDFLAGS = -F$(HOME)/Library/Frameworks -framework gtest \
	-L$(HOME)/Library/Frameworks/gtest.Framework/Resources \
	-stdlib=libc++ \
	-lgtest_main

SOURCES=$(wildcard *.cpp)
OBJECTS=$(patsubst %.cpp,%.o,$(SOURCES))
