<?xml version="1.0"?>
<root>

  <!--
      | <modifierdef> defines a modifier.
      | For example, <modifierdef>EXTRA1</modifierdef> defines ModifierFlag and KeyCodes.
      |
      | * ModifierFlag::EXTRA1
      | * KeyCode::VK_MODIFIER_EXTRA1
      | * KeyCode::VK_LOCK_EXTRA1
      | * KeyCode::VK_LOCK_EXTRA1_FORCE_ON
      | * KeyCode::VK_LOCK_EXTRA1_FORCE_OFF
      | * KeyCode::VK_STICKY_EXTRA1
      | * KeyCode::VK_STICKY_EXTRA1_FORCE_ON
      | * KeyCode::VK_STICKY_EXTRA1_FORCE_OFF
      |
      |
      | You can use new modifier like this:
      |
      | * Change the right command key to the extra1 modifier key:
      |
      |   <autogen>
      |     __KeyToKey__
      |     KeyCode::COMMAND_R,
      |     KeyCode::VK_MODIFIER_EXTRA1,
      |   </autogen>
      |
      | * Change extra1+keys to other keys:
      |
      |   <autogen>
      |     __KeyToKey__
      |     KeyCode::A, ModifierFlag::EXTRA1,
      |     KeyCode::KEY_1,
      |   </autogen>
      |   <autogen>
      |     __KeyToKey__
      |     KeyCode::S, ModifierFlag::EXTRA1,
      |     KeyCode::KEY_2,
      |   </autogen>
      |
  -->

  <modifierdef>EXTRA1</modifierdef>
  <modifierdef>EXTRA2</modifierdef>
  <modifierdef>EXTRA3</modifierdef>
  <modifierdef>EXTRA4</modifierdef>
  <modifierdef>EXTRA5</modifierdef>
  <modifierdef>EXTRA6</modifierdef>
  <modifierdef>EXTRA7</modifierdef>
  <modifierdef>EXTRA8</modifierdef>
  <modifierdef>EXTRA9</modifierdef>
  <modifierdef>EXTRA10</modifierdef>

  <!-- For "Expose Mode 2" -->
  <modifierdef>EXPOSE_MODE_2</modifierdef>

  <!-- The lock and sticky modifier notification will be hidden if you set notify="false". -->
  <modifierdef notify="false">A</modifierdef>
  <modifierdef notify="false">B</modifierdef>
  <modifierdef notify="false">C</modifierdef>
  <modifierdef notify="false">D</modifierdef>
  <modifierdef notify="false">E</modifierdef>
  <modifierdef notify="false">F</modifierdef>
  <modifierdef notify="false">G</modifierdef>
  <modifierdef notify="false">H</modifierdef>
  <modifierdef notify="false">I</modifierdef>
  <modifierdef notify="false">J</modifierdef>
  <modifierdef notify="false">K</modifierdef>
  <modifierdef notify="false">L</modifierdef>
  <modifierdef notify="false">M</modifierdef>
  <modifierdef notify="false">N</modifierdef>
  <modifierdef notify="false">O</modifierdef>
  <modifierdef notify="false">P</modifierdef>
  <modifierdef notify="false">Q</modifierdef>
  <modifierdef notify="false">R</modifierdef>
  <modifierdef notify="false">S</modifierdef>
  <modifierdef notify="false">T</modifierdef>
  <modifierdef notify="false">U</modifierdef>
  <modifierdef notify="false">V</modifierdef>
  <modifierdef notify="false">W</modifierdef>
  <modifierdef notify="false">X</modifierdef>
  <modifierdef notify="false">Y</modifierdef>
  <modifierdef notify="false">Z</modifierdef>
  <modifierdef notify="false">DELETE</modifierdef>
  <modifierdef notify="false">ESCAPE</modifierdef>
  <modifierdef notify="false">RETURN</modifierdef>
  <modifierdef notify="false">SPACE</modifierdef>
  <modifierdef notify="false">TAB</modifierdef>
</root>
